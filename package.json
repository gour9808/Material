{
  "name": "carbook-fleet-management",
  "version": "1.8.0",
  "license": "MIT",
  "scripts": {
    "ng-high-mem": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "start:prod": "ng serve --environment=prod",
    "latest:dev": "git fetch && git pull origin development",
    "latest:prod": "git fetch && git pull origin master",
    "START_PRODUCTION_SCRIPT": "-x-x-x-x-x-x-x-x-x-x-x-x-x-x--x-x-x-x-x-x-x-x-x-x-x-x-x-x-x-",
    "build": "npm run deploy:build && npm run deploy:builddock && npm run deploy:pushdock && npm run deploy:marathon",
    "deploy:build": "ng build --base-href=/carbook-fleet-management/ --environment=prod -prod --build-optimizer -oh all",
    "deploy:builddock": "cross-var docker build -t docker-reg.widas.de/carbook-fleet-management:vP$npm_package_version-$(git rev-parse --short HEAD) .",
    "deploy:pushdock": "cross-var docker push docker-reg.widas.de/carbook-fleet-management:vP$npm_package_version-$(git rev-parse --short HEAD)",
    "deploy:marathon": "node deployment/deploy-prod.js",
    "END_PRODUCTION_SCRIPT": "-x-x-x-x-x-x-x-x-x-x-x-x-x-x--x-x-x-x-x-x-x-x-x-x-x-x-x-x-x-",
    "START_TEST_SCRIPT": "-x-x-x-x-x-x-x-x-x-x-x-x-x-x--x-x-x-x-x-x-x-x-x-x-x-x-x-x-x-",
    "test": "npm run test:build && npm run test:builddock && npm run test:pushdock && npm run test:marathon",
    "test:build": "npm run ng-high-mem -- build --base-href=/carbook-fleet-management/ --environment=test --aot --build-optimizer -oh all",
    "test:builddock": "cross-var docker build -t docker-reg.widas.de/carbook-fleet-management:v$npm_package_version-$(git rev-parse --short HEAD) .",
    "test:pushdock": "cross-var docker push docker-reg.widas.de/carbook-fleet-management:v$npm_package_version-$(git rev-parse --short HEAD)",
    "test:marathon": "node deployment/deploy-test.js",
    "END_TEST_SCRIPT": "-x-x-x-x-x-x-x-x-x-x-x-x-x-x--x-x-x-x-x-x-x-x-x-x-x-x-x-x-x-",
    "START_RELEASE_SCRIPT": "-x-x-x-x-x-x-x-x-x-x-x-x-x-x--x-x-x-x-x-x-x-x-x-x-x-x-x-x-x-",
    "release": "standard-version",
    "release-major": "npm run release -- --release-as major",
    "release-minor": "npm run release -- --release-as minor",
    "release-patch": "npm run release -- --release-as patch",
    "release-beta": "npm run release -- --prerelease beta",
    "release-alpha": "npm run release -- --prerelease alpha",
    "END_RELEASE_SCRIPT": "-x-x-x-x-x-x-x-x-x-x-x-x-x-x--x-x-x-x-x-x-x-x-x-x-x-x-x-x-x-",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -s --port 4600",
    "coverage": "ng test --watch=false --code-coverage"
  },
